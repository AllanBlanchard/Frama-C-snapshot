case $OCAMLVERSION in
  3.09.3*) echo "${ECHO_T}Good!";;
  3.09*) echo "${ECHO_T}Warning: unsupported version.";
         if test "$UNSUPPORTED_OCAML" == "yes"; then
           echo "Compile at your own risks";
