new_section() {
  banner=`echo "* $1 *" | sed -e 's/./*/g'`
  title=`echo "* $1 *" | tr "a-z" "A-Z"`
  { echo "$as_me:$LINENO: $banner" >&5
echo "$as_me: $banner" >&6;}
  { echo "$as_me:$LINENO: $title" >&5
echo "$as_me: $title" >&6;}
  { echo "$as_me:$LINENO: $banner" >&5
echo "$as_me: $banner" >&6;}
}

